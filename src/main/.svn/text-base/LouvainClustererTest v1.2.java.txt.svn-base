            boolean isContained = false;
            if (hierSet.contains(bioObj)){
                    return true;
            }
            for (Object obj : hierSet){
                if (obj instanceof Set){
                    
                    
                    isContained |= isContainedIn(bioObj,(Set) obj);   
                }
            }
            return isContained;
        }
